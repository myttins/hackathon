{
  "manifest_version": 3,

  "name": "Extension Name",
  "description": "description",
  "version": "1.0",

  "action": {
    "default_icon" : "icon.png",
    "default_popup" : "index.html"
  }, 
  "permissions" : [
    "activeTab",
    "contextMenus",
    "storage"
  ],


  "content_scripts" :  [
    {
        "js" : ["script.js"],
        "matches" : ["<all_urls>"]
    }
    ]

}